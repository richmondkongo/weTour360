{
  "compileOnSave": false,
  "compilerOptions": {
	  "paths": { "*": ["types/*"] },
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types",
      "node_modules/@types/jquery",
      "leaflet",
      "aframe",
      "jquery",
      "materialize-css"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
